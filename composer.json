{
	"name": "apigen/apigen",
	"description": "PHP source code API generator",
	"type": "project",
	"keywords": ["documentation", "phpdoc", "generator", "api"],
	"homepage": "http://apigen.org/",
	"license": "MIT",
	"authors": [
		{
			"name": "Tomáš Votruba",
			"email": "tomas.vot@gmail.com"
		},
		{
			"name": "Olivier Laviale",
			"homepage": "https://github.com/olvlvl"
		},
		{	"name": "Jaroslav Hanslík",
			"homepage": "https://github.com/kukulich"
		},
		{
			"name": "Ondřej Nešpor",
			"homepage": "https://github.com/andrewsville"
		},
		{
			"name": "David Grudl",
			"homepage": "http://davidgrudl.com"
		}
	],
	"require": {
		"php": ">=5.3.1",
		"nette/bootstrap": "~2.2",
		"nette/di": "~2.2",
		"nette/mail": "~2.2",
		"nette/robot-loader": "~2.2",
		"nette/safe-stream": "~2.2",
		"latte/latte": "~2.2",
		"tracy/tracy": "~2.2",

		"kukulich/fshl": "~2.1",
		"andrewsville/php-token-reflection": "~1.4",
		"michelf/php-markdown": "~1.4",
		"kdyby/events": "~2.0",
		"kdyby/console": "~2.0",

		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-iconv": "*",
		"ext-tokenizer": "*"
	},
	"require-dev": {
		"nette/tester": "~1.2",
		"jakub-onderka/php-parallel-lint": "~0.7"
	},
	"suggest": {
		"ext-bz2": "*",
		"ext-phar": "*",
		"ext-zip": "*",
		"ext-zlib": "*"
	},
	"autoload": {
		"psr-0": {
			"ApiGen": "src/"
		}
	},
	"bin": [
		"bin/apigen"
	],
	"extra": {
		"branch-alias": {
			"dev-develop": "3.0.0-dev"
		}
	}
}
